<?xml version="1.0" encoding="UTF-8"?>
<externaltool id="Compile_SecreC">
    <description>Compile a SecreC file.</description>
    <displayname>Compile SecreC</displayname>
    <category>Sharemind SDK</category>
    <executable output="showinpane" error="showinpane" modifiesdocument="yes">
        <path>sm_compile.sh</path>
        <arguments>%{CurrentDocument:FilePath}</arguments>
        <workingdirectory>%{CurrentDocument:Path}</workingdirectory>
        <environment>PATH%3D%24%7BPATH%7D%3A%2Fusr%2Flocal%2Fbin%3A%2Fusr%2Flocal%2Fsharemind%2Fbin</environment>
    </executable>
</externaltool>
